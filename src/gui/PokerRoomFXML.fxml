<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" fx:id="pokerPane" prefHeight="480.0" prefWidth="720.0" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.PokerRoomFXMLController">
   <children>
      <Label layoutX="79.0" layoutY="59.0" text="Your Cards:">
         <font>
            <Font name="Gill Sans MT" size="34.0" />
         </font>
      </Label>
      <Label layoutX="388.0" layoutY="59.0" text="Computer's Cards:">
         <font>
            <Font name="Gill Sans MT" size="34.0" />
         </font>
      </Label>
      <Label fx:id="playerCardsList" layoutX="154.0" layoutY="129.0">
         <font>
            <Font name="Gill Sans MT" size="16.0" />
         </font>
      </Label>
      <Label fx:id="computerCardsList" layoutX="504.0" layoutY="139.0">
         <font>
            <Font name="Gill Sans MT" size="16.0" />
         </font>
      </Label>
      <Button fx:id="dealHandsButton" layoutX="102.0" layoutY="368.0" mnemonicParsing="false" onAction="#dealHands" text="Deal Hands">
         <font>
            <Font name="Gill Sans MT" size="20.0" />
         </font>
      </Button>
      <Button fx:id="findWinnerButton" layoutX="427.0" layoutY="368.0" mnemonicParsing="false" onAction="#determineWinner" text="Determine Winner">
         <font>
            <Font name="Gill Sans MT" size="20.0" />
         </font>
      </Button>
      <Label layoutX="292.0" layoutY="179.0" text="Common Cards:">
         <font>
            <Font name="Gill Sans MT" size="20.0" />
         </font>
      </Label>
      <Label fx:id="commonCardsLabel" layoutX="346.0" layoutY="232.0">
         <font>
            <Font name="Gill Sans MT" size="16.0" />
         </font></Label>
   </children>
</AnchorPane>
